<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>AddEmployee</servlet-name>
		<display-name>AddEmployee</display-name>
		<description></description>
		<servlet-class>com.Employee.servlets.AddEmployee</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EmployeeList</servlet-name>
		<display-name>EmployeeList</display-name>
		<description></description>
		<servlet-class>com.Employee.servlets.EmployeeList</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteEmployee</servlet-name>
		<display-name>DeleteEmployee</display-name>
		<description></description>
		<servlet-class>com.Employee.servlets.DeleteEmployee</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddEmployee</servlet-name>
		<url-pattern>/AddEmployee</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EmployeeList</servlet-name>
		<url-pattern>/EmployeeList</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteEmployee</servlet-name>
		<url-pattern>/DeleteEmployee</url-pattern>
	</servlet-mapping>
 
	<welcome-file-list>
		<welcome-file>employeeForm.jsp</welcome-file>
	</welcome-file-list>
 
</web-app>
 
  
   
 